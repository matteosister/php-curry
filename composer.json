{
    "name": "cypresslab/php-curry",
    "description": "Curried functions in PHP",
    "keywords": ["curry", "functional-programming"],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Cypress\\Curry\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}

